<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background">


    <ImageButton
        android:id="@+id/TT"
        android:layout_width="382dp"
        android:layout_height="292dp"
        android:layout_gravity="center"
        android:layout_marginTop="16dp"
        android:background="#00000000"
        android:foreground="@drawable/taco2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/taco2" />

    <ImageButton
        android:id="@+id/U5"
        android:layout_width="381dp"
        android:layout_height="71dp"
        android:layout_marginBottom="20dp"
        android:background="#00000000"
        android:foreground="@drawable/image_frame"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@drawable/image_frame" />

    <ImageButton
        android:id="@+id/U4"
        android:layout_width="381dp"
        android:layout_height="71dp"
        android:layout_marginBottom="5dp"
        android:foreground="@drawable/image_frame"
        app:layout_constraintBottom_toTopOf="@+id/U5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@drawable/image_frame" />

    <ImageButton
        android:id="@+id/U3"
        android:layout_width="381dp"
        android:layout_height="71dp"
        android:layout_marginBottom="5dp"
        android:foreground="@drawable/image_frame"
        app:layout_constraintBottom_toTopOf="@+id/U4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@drawable/image_frame" />

    <ImageButton
        android:id="@+id/U2"
        android:layout_width="381dp"
        android:layout_height="71dp"
        android:foreground="@drawable/image_frame"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/U3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@drawable/image_frame" />

    <ImageButton
        android:id="@+id/U1"
        android:layout_width="381dp"
        android:layout_height="71dp"

        android:foreground="@drawable/image_frame"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/U2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@drawable/image_frame" />

    <TextView
        android:id="@+id/TCD"
        android:layout_width="123dp"
        android:layout_height="48dp"
        android:layout_marginTop="60dp"
        android:background="#f9fcb8"
        android:text="####"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/RD"
        android:layout_width="123dp"
        android:layout_height="48dp"
        android:layout_marginTop="60dp"
        android:background="#f9fcb8"
        android:text="####"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageViewFlipFlops"
        android:layout_width="37dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="308dp"
        android:adjustViewBounds="true"
        android:foreground="@drawable/flipflop"
        android:scaleType="center"
        app:layout_constraintEnd_toEndOf="@+id/U1"
        app:layout_constraintTop_toTopOf="@+id/U1" />

    <ImageView
        android:id="@+id/imageViewPapaLouis"
        android:layout_width="119dp"
        android:layout_height="105dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="260dp"
        android:adjustViewBounds="true"
        android:foreground="@drawable/gandd"
        android:scaleType="center"
        app:layout_constraintEnd_toEndOf="@+id/U2"
        app:layout_constraintTop_toTopOf="@+id/U1" />

    <ImageView
        android:id="@+id/imageViewChiken"
        android:layout_width="50dp"
        android:layout_height="53dp"
        android:layout_marginStart="32dp"
        android:adjustViewBounds="true"
        android:foreground="@drawable/chiken4"
        android:scaleType="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/U3"
        app:layout_constraintTop_toTopOf="@+id/U3"
        app:layout_constraintVertical_bias="0.05" />

    <ImageView
        android:id="@+id/imageViewPapaLouisName"
        android:layout_width="143dp"
        android:layout_height="31dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="6dp"
        android:adjustViewBounds="true"
        android:foreground="@drawable/chikkken_label"
        android:scaleType="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/U3"
        app:layout_constraintTop_toTopOf="@+id/U3"
        app:layout_constraintVertical_bias="0.018" />

    <ImageView
        android:id="@+id/imageViewPinaple"
        android:layout_width="68dp"
        android:layout_height="78dp"
        android:layout_marginTop="71dp"
        android:layout_marginEnd="290dp"

        android:adjustViewBounds="true"
        android:foreground="@drawable/pinaple"
        android:scaleType="center"
        app:layout_constraintEnd_toEndOf="@+id/U4"
        app:layout_constraintTop_toTopOf="@+id/U3" />

    <ImageView
        android:id="@+id/imageViewDragon"
        android:layout_width="137dp"
        android:layout_height="92dp"

        android:layout_marginTop="71dp"
        android:layout_marginEnd="243dp"
        android:adjustViewBounds="true"
        android:foreground="@drawable/dragon"
        android:scaleType="center"
        app:layout_constraintEnd_toEndOf="@+id/U5"
        app:layout_constraintTop_toTopOf="@+id/U4" />
</androidx.constraintlayout.widget.ConstraintLayout>